@article{chan_fencing_2011,
	title = {Fencing expertise and physical fitness enhance action inhibition},
	volume = {12},
	copyright = {https://www.elsevier.com/tdm/userlicense/1.0/},
	issn = {14690292},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S1469029211000549},
	doi = {10.1016/j.psychsport.2011.04.006},
	abstract = {Objective: This study investigated the effects of fencing expertise and physical ﬁtness on the inhibitory control of fencers and non-fencers. Design: This study used a 2 2 factorial design. Fencers and non-fencers both in low-ﬁt and averagely-ﬁt subgroups were compared in reaction times (RT) and accuracy in simple reaction time (SRT) and go/nogo reaction time (go/no-go RT) tasks.
Method: The participants were 30 fencers (aged 18e26) and 30 non-fencers (aged 19e25), each having a different ﬁtness level. With a standard computer keyboard, each participant performed an SRT task by responding to all stimuli. In the go/no-go RT task, each participant responded only to the go signals while withholding their response to the no-go signals.
Results: There were no signiﬁcant differences between the participants with different levels of ﬁtness or fencing expertise in SRT, go/no-go RT, omission error and commission error. However, an interaction of ﬁtness and fencing expertise on commission error was found (p {\textless} .05). Averagely-ﬁt fencers committed a similar number of errors to the averagely-ﬁt non-fencers, but the high-ﬁt fencers committed signiﬁcantly fewer errors compared to the high-ﬁt non-fencers (p {\textless} .05).
Conclusions: Fencing experience and physical ﬁtness facilitate a person’s ability to withhold action when necessary. The interactive nature of aerobic ﬁtness and sport expertise on action inhibition suggests that cognitive control beneﬁts most from the combination of physical and mental training compared to when each is administered singly.},
	language = {en},
	number = {5},
	urldate = {2025-08-20},
	journal = {Psychology of Sport and Exercise},
	author = {Chan, John S.Y. and Wong, Alan C.N. and Liu, Yu and Yu, Jie and Yan, Jin H.},
	month = sep,
	year = {2011},
	pages = {509--514},
	file = {Chan et al. - 2011 - Fencing expertise and physical fitness enhance action inhibition:D\:\\work\\db-transfer\\Zotero\\storage\\YKVMNREK\\Chan et al. - 2011 - Fencing expertise and physical fitness enhance action inhibition.txt:text/plain;PDF:D\:\\work\\db-transfer\\Zotero\\storage\\IMDQ7I8G\\Chan et al. - 2011 - Fencing expertise and physical fitness enhance action inhibition.pdf:application/pdf},
}

